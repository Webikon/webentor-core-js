{
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@webentorCore",
    "^@webentorCore/(.*)$",
    "",
    "^@scripts",
    "^@scripts/(.*)$",
    "^[.]",
    "",
    "^@images",
    "^@images/(.*)$",
    "",
    "^@fonts",
    "^@fonts/(.*)$",
    "",
    "^@styles",
    "^@styles/(.*)$",
    "",
    "<TYPES>^[.]",
    "<TYPES>",
    "^@types",
    "^@types/(.*)$",
    "",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
