{
  "name": "webentor-core-js",
  "description": "Core JS functionality and useful utilities for Webentor Stack.",
  "homepage": "https://webikon.sk",
  "version": "0.9.2",
  "license": "MIT",
  "author": "Webikon s.r.o.",
  "maintainers": [
    {
      "name": "Webikon s.r.o.",
      "email": "support@webikon.sk"
    },
    {
      "name": "Pavol Caban",
      "email": "pavol.caban@webikon.sk"
    }
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src",
    "prettier-check": "prettier --check 'src/**/*.{js,ts,tsx,scss,css}'",
    "prettier-check:js": "prettier --check 'src/**/*.{js,ts,tsx}'",
    "prettier-check:css": "prettier --check 'src/**/*.css'",
    "prettier-fix": "prettier --write 'src/**/*.{js,ts,tsx,css}'",
    "prettier-fix:js": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "prettier-fix:css": "prettier --write 'src/**/*.css'",
    "compile": "tsc --project tsconfig.json"
  },
  "devDependencies": {
    "@10up/block-components": "^1.20.0",
    "@eslint/js": "^9.23.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/wordpress__block-editor": "^11.5.16",
    "@wordpress/block-editor": "^14.15.0",
    "@wordpress/block-library": "^9.20.0",
    "@wordpress/blocks": "^14.9.0",
    "@wordpress/components": "^29.6.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^6.20.0",
    "@wordpress/i18n": "^5.20.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "prettier": "^3.3.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint:js --"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.14.9",
    "alpinejs": "^3.14.9",
    "swiper": "^11.2.6"
  },
  "peerDependencies": {
    "@alpinejs/collapse": "^3.14.9",
    "alpinejs": "^3.14.9",
    "swiper": "^11.2.6"
  }
